{
  "name": "material-kit-react",
  "version": "1.0.0",
  "private": false,
  "dependencies": {
    "@material-ui/icons": "1.0.0-beta.43",
    "ajv": "^6.5.0",
    "build-css": "^0.2.0",
    "discord.js": "^11.4.2",
    "firebase": "^5.0.4",
    "firebase-tools": "^3.19.3",
    "history": "4.7.2",
    "install": "^0.11.0",
    "material-ui": "^1.0.0-beta.46",
    "material-ui-icons": "^1.0.0-beta.36",
    "moment": "2.22.1",
    "node-opus": "^0.3.1",
    "node-sass-chokidar": "^1.3.0",
    "npm": "^6.2.0",
    "npm-run-all": "^4.1.5",
    "package.json": "^2.0.1",
    "react": "16.3.1",
    "react-beautiful-dnd": "^7.1.3",
    "react-datetime": "2.14.0",
    "react-dom": "16.3.1",
    "react-nouislider": "2.0.1",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.2.2",
    "react-scripts": "^1.1.4",
    "react-slick": "0.23.1",
    "react-swipeable-views": "0.12.13",
    "styled-components": "^3.3.0"
  },
  "scripts": {
    "build-css": "node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/ --watch --recursive --host 0.0.0.0",
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build": "npm run build-css && react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  }
}
